{
    "expo": {
      "name": "Gilli-Danda",
      "slug": "Gilli-Danda",
      "platforms": [
        "ios",
        "android",
        "web"
      ],
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/icon.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true,
        "bundleIdentifier": "com.gillidanda"
      },
      "android": {
        "package": "com.gillidanda"
      },
      "entryPoint": "node_modules/expo/AppEntry.js"
    }
  }
  